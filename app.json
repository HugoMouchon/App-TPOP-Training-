{
  "expo": {
    "name": "TPOP Reco",
    "slug": "TPOP Reco",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/TPOP.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo/TPOP.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "androidNavigationBar": {
      "backgroundColor": "#fff7dd",
      "barStyle": "light-content"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/TPOP.png",
        "backgroundColor": "black"
      }
    },
    "web": {
      "favicon": "./assets/logo/TPOP.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
